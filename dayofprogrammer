    static String dayOfProgrammer(int year) {
        //stuff
        boolean leap = false;
        boolean pre = (year>=1700 && year<=1917) ? true : false;
        
        if(pre) leap = year%4==0;
        else leap = year%400 == 0 ^ (year%4 == 0 && year%100!=0);
        
        //return stuff
        if(year==1918) return "26.09.1918";
        if(leap) return "12.09." + year;
        return "13.09." + year;
    }
